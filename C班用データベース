-- データベースを使う
use sample1;

-- テーブルmembersの作成
drop table if exists members;-- 一度削除する

create table members (
  id int primary key,-- idは整数で主キー
  name varchar(50),  -- nameは文字列（最大50文字）
  team_id int         -- 研究室のID（面倒になるから外部キーは設定しない）
);

-- データの追加
insert into members (id, name, team_id) values (1, '岩瀬', 1);
insert into members (id, name, team_id) values (2, '岩橋', 2);
insert into members (id, name, team_id) values (3, '大木', 1);
insert into members (id, name, team_id) values (4, '小山', 1);
insert into members (id, name, team_id) values (5, 'Alice', 2);

-- テーブルlaboratoriesの作成
drop table if exists laboratories;-- 一度削除する

create table labos (
  name varchar(50),
  id int primary key-- idは整数で主キー
);

